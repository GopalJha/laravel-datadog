{
    "name": "gopaljha/laravel-datadog",
    "description": "A simple package to wrap DataDog API for Timeseries metrics",
    "type": "package",
    "keywords": [
        "laravel",
        "datadog"
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": "5.3.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "GopalJha",
            "email": "gopalpsdjha89@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "GopalJha\\LaravelDataDog\\LaravelDataDogServiceProvider"
            ],
            "aliases": {
                "Debugbar": "GopalJha\\LaravelDataDog\\Facades\\DataDog"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "GopalJha\\LaravelDataDog\\": "src/"
        }
    }
}